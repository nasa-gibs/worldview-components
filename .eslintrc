{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "rules": {
    "quotes": [ "warn", "single" ],
    "react/no-danger": "warn",
    "react/prop-types": "warn",
    "react/no-did-update-set-state": "warn"
  }
}
